page {
  background-color: #F2F3F4;
}
.header {
  background-color: #FFFFFF;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.header .photo {
  padding: 75rpx 50rpx;
}
.header .photo image {
  width: 150rpx;
  height: 150rpx;
  border-radius: 10rpx;
}
.header .info {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.header .info #name {
  font-size: 50rpx;
}
.header .info #ID {
  color: gray;
}
.icon-tuichu {
  color: #d80000;
}
.icon-gerenxinxi_o {
  color: #00b26a;
}
.icon-shezhi {
  color: blue;
}
.item {
  background-color: #FFFFFF;
  margin: 28rpx 0;
  height: 100rpx;
  line-height: 100rpx;
}
.item .icon_1 {
  width: 75rpx;
  font-size: 60rpx;
  float: left;
  margin-left: 50rpx;
}
.item .icon_2 {
  width: 75rpx;
  float: right;
}
.item .des {
  overflow: hidden;
  padding-left: 20rpx;
}

